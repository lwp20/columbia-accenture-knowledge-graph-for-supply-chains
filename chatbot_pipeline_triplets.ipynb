    "        answers.append(output_text.strip())\n",
    "    id=start_idx\n",
    "    with open(model_name+\"_\"+\"answers.txt\", \"a\",encoding='utf-8') as file:\n",
    "        for answer in answers:\n",
    "            file.write(str(id)+\"<SEP>\" '\\n')\n",
    "            file.write(answer + '\\n')\n",
    "            id=id+1\n",
    "\n",
